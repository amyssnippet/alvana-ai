{
  "expo": {
    "name": "A1vana",
    "slug": "alvana-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/alvana.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/alvana.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.amol_6125.alvanaapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/nobg.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/alvana.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fbefbf60-dd28-484f-8765-3637049d27fa"
      }
    }
  }
}